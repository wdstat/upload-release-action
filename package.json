{
  "name": "upload-release-action",
  "version": "2.3.0",
  "private": true,
  "description": "Upload files to a GitHub release",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svestaro/upload-release-action.git"
  },
  "keywords": [
    "actions",
    "upload",
    "release",
    "assets",
    "asset"
  ],
  "author": "Sven-Hendrik Haase",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^19.0.5",
    "@types/glob": "^8.0.0",
    "glob": "^8.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@typescript-eslint/parser": "^5.46.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.29.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
